{
  "manifest_version": 3,
  "name": "no-ads",
  "version": "1.0",
  "description": "The extension that hides adds",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/content.js"
      ]
    }
  ]
}